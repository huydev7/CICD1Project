pipeline {

    agent any
    stages {
        stage('Packaging/Pushing imagae') {
            steps {
                withDockerRegistry(credentialsId: 'dockerhub', url: 'https://index.docker.io/v1/') {
                    // sh 'docker compose up -d --build'
                    // sh 'docker compose push'
                    sh 'ls'
                }
            }
        }
    }
    post {
        // Clean after build
        always {
            cleanWs()
        }
    }
}
